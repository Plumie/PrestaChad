{
  "name": "prestachad",
  "version": "1.0.0",
  "description": "Modern theme boilerplate for PrestaShop",
  "main": "index.js",
  "scripts": {
    "scss-lint": "stylelint \"**/*.scss\" --formatter verbose",
    "scss-fix": "stylelint \"**/*.scss\" --fix",
    "lint": "eslint -c .eslintrc.js --ext .js,.vue ./js",
    "lint-fix": "eslint -c .eslintrc.js --ext .js,.vue ./js --fix",
    "storybook:serve": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "vite:watch": "npm run vite:build:theme && npm run vite:watch:custom",
    "vite:watch:theme": "vite build -c vite.theme.config.js --watch -m development",
    "vite:watch:custom": "vite build -c vite.custom.config.js --watch -m development",
    "vite:build": "npm run vite:build:theme && npm run vite:build:custom",
    "vite:build:theme": "vite build -c vite.theme.config.js",
    "vite:build:custom": "vite build -c vite.custom.config.js"
  },
  "author": "PrestaShop",
  "license": "AFL-3.0",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.18.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/html": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "bootstrap": "4.0.0-alpha.5",
    "bootstrap-touchspin": "^3.1.1",
    "bourbon": "^4.2.6",
    "browser-sync": "^2.27.11",
    "css-loader": "^6.7.1",
    "csso-webpack-plugin": "^2.0.0-beta.3",
    "esbuild-loader": "^2.15.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.0",
    "expose-loader": "^3.0.0",
    "flexibility": "^1.0.5",
    "jquery": "^3.5.1",
    "jquery-touchswipe": "^1.6",
    "jquery.browser": "^0.1.0",
    "manrope": "^2.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^7.0.3",
    "postcss": "^8.4.21",
    "postcss-flexibility": "^3.0.0",
    "postcss-loader": "^6.1.1",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.2.1",
    "stylelint": "^12.0.0",
    "stylelint-config-prestashop": "^1.0.5",
    "tailwindcss": "^3.2.7",
    "tether": "^1.1.1",
    "velocity-animate": "^1.2.3",
    "vite-plugin-browser-sync": "^1.0.2",
    "vite-plugin-externals": "^0.6.2"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "jsmart": "^4.0.0",
    "jsmart-loader": "^2.0.0",
    "prestashop": "^0.0.1",
    "sprintf-js": "^1.1.2",
    "storybook-addon-sass-postcss": "^0.1.3",
    "vite": "^4.1.3"
  }
}
